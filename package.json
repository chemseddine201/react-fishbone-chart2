{
    "name": "react-fishbone-chart2",
    "version": "1.0.23",
    "description": "reactjs fishbone chart component ",
    "license": "MIT",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/esm/index.d.ts",
    "scripts": {
        "prepublishOnly": "npm run build",
        "test": "cross-env CI=1 react-scripts test --env=jsdom",
        "test:watch": "react-scripts test --env=jsdom",
        "start": "npm run copy-css && tsc --watch",
        "prepare": "npm run build",
        "dopublish": "npm run build && npm publish",
        "predeploy": "cd example && npm install && npm run build",
        "deploy": "gh-pages -d example/build",
        "build": "rm -rf ./dist && npm run build:esm && npm run build:cjs",
        "build:esm": "tsc && npm run copy-css",
        "build:cjs": "tsc --module commonjs --outDir dist/cjs",
        "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
        "copy-css": "node scripts/copy-css.js",
        "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\""
    },
    "peerDependencies": {
        "react": ">=16.8.0",
        "react-dom": ">=16.8.0"
    },
    "devDependencies": {
        "@babel/plugin-transform-react-constant-elements": "^7.25.9",
        "@babel/preset-env": "^7.26.0",
        "@babel/preset-react": "^7.25.9",
        "@rollup/plugin-babel": "^6.0.4",
        "@rollup/plugin-commonjs": "^28.0.1",
        "@rollup/plugin-multi-entry": "^6.0.1",
        "@rollup/plugin-node-resolve": "^15.3.0",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^12.1.1",
        "@svgr/rollup": "^8.1.0",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.0.1",
        "@types/fs-extra": "^11.0.4",
        "@types/node": "^22.10.1",
        "@types/react": "^18.3.13",
        "@types/react-dom": "^18.3.1",
        "@typescript-eslint/eslint-plugin": "^8.17.0",
        "@typescript-eslint/parser": "^8.17.0",
        "autoprefixer": "^10.4.20",
        "babel-core": "^6.26.3",
        "babel-plugin-external-helpers": "^6.22.0",
        "copyfiles": "^2.4.1",
        "cssnano": "^7.0.6",
        "eslint": "^9.16.0",
        "eslint-plugin-react": "^7.37.2",
        "eslint-plugin-react-hooks": "^5.0.0",
        "fs-extra": "^11.2.0",
        "jest": "^29.7.0",
        "postcss": "^8.4.49",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "rollup": "^4.28.0",
        "rollup-plugin-babel": "^4.4.0",
        "rollup-plugin-dts": "^6.1.1",
        "rollup-plugin-ignore": "^1.0.10",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-postcss": "^4.0.2",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-url": "^3.0.1",
        "ts-jest": "^29.2.5",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "tsc-alias": "^1.8.10",
        "tslib": "^2.8.1",
        "typescript": "^5.7.2",
        "vite": "^6.0.2"
    },
    "dependencies": {
        "@babel/core": "^7.26.0",
        "@rollup/plugin-replace": "^6.0.1",
        "babel-loader": "^9.2.1",
        "css-loader": "^7.1.2",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-css-only": "^4.5.2",
        "rollup-plugin-node-resolve": "^5.2.0",
        "run-script-os": "^1.1.6",
        "style-loader": "^4.0.0"
    },
    "files": [
        "dist"
